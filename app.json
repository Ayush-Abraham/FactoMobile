{
  "expo": {
    "name": "FactoMobile",
    "slug": "FactoMobile",
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/history.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/history.png",
        "backgroundColor": "#FFFFFF",
      },
      "package": "com.ayushabraham.factomobile"
    },
    "web": {
      "favicon": "./assets/history.png"
    }
  }
}
